{
 "manifest_version": 2,
 "name": "Chrome Time",
 "version": "1.0.0",
 "description": "Extension for short timers and to meet the timelines..",
 "short_name": "Chrome Time",
 "permissions": ["activeTab", "declarativeContent", "<all_urls>"],
 "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "css": ["background.css"],
     "js": ["background.js"]
   }
 ],
 "browser_action": {
   "default_title": "Set and Start a timer",
   "default_popup": "popup.html",
   "default_icon": {
     "16": "icons/icon16.png",
     "32": "icons/icon32.png"
   }
 }
}
